<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name></display-name>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>LoginAction</servlet-name>
    <servlet-class>com.login.manager.LoginAction</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>SignUpAction</servlet-name>
    <servlet-class>com.login.manager.SignUpAction</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>MessageAction</servlet-name>
    <servlet-class>com.login.manager.MessageAction</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>InsertAction</servlet-name>
    <servlet-class>com.login.manager.InsertAction</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>TrackmapAction</servlet-name>
    <servlet-class>momenthere.trackmap.TrackmapAction</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>LoginAction</servlet-name>
    <url-pattern>/servlet/LoginAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SignUpAction</servlet-name>
    <url-pattern>/servlet/SignUpAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MessageAction</servlet-name>
    <url-pattern>/servlet/MessageAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>InsertAction</servlet-name>
    <url-pattern>/servlet/InsertAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TrackmapAction</servlet-name>
    <url-pattern>/servlet/TrackmapAction</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>